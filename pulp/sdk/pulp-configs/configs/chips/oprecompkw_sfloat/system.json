{
  "system_tree": {
    "includes": [
      "pulp_system_common.json"
    ],
    "vp_class": "pulp/system",
    "vp_comps": [
      "board"
    ],
    "debug_bridge": {},
    "board": {
      "vp_class": "pulp/board",
      "vp_comps": [
        "chip",
        "dpi_clock",
        "pulp_chip",
        "dpi",
        "ddr_clock",
        "ddr"
      ],
      "vp_bindings": [
        [
          "chip->ddr",
          "ddr->input"
        ],
        [
          "dpi_clock->out",
          "dpi->clock"
        ],
        [
          "ddr_clock->out",
          "ddr->clock"
        ]
      ],
      "chip": {
        "includes": [
          "chip.json"
        ],
        "vp_ports": [
          "ddr"
        ]
      },
      "dpi_clock": {
        "vp_class": "vp/clock_domain",
        "frequency": 50000000,
        "vp_ports": [
          "out"
        ]
      },
      "pulp_chip": {
        "oprecompkw_sfloat": {}
      },
      "dpi": {
        "includes": [
          "periph/dpi_wrapper.json"
        ],
        "vp_ports": [
          "clock"
        ]
      },
      "ddr_clock": {
        "vp_class": "vp/clock_domain",
        "frequency": null,
        "vp_ports": [
          "out"
        ]
      },
      "ddr": {
        "includes": [
          "ips/memory/ddr.json"
        ],
        "size": "0x10000000",
        "vp_ports": [
          "input",
          "clock"
        ]
      }
    }
  }
}
