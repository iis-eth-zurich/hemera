      [
        "self->debug",
        "ll_ico->input"
      ],
      [
        "self->axi_slave",
        "ll_ico->input"
      ],
      [
        "ll_ico->apb",
        "self->apb"
      ],
      [
        "ll_ico->l2",
        "self->l2"
      ],
      [
        "ll_ico->axi_master",
        "self->axi_master"
