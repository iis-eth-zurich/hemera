    "mappings": {
      "soc": {
        "base": "0x1A000000",
        "size": "0x06000000"
      },
      "cluster": {
        "base": "0x10000000",
        "size": "0x400000"
      }
    },
    "vp_ports": [
      "cluster",
      "soc",
      "input"
    ]
  },
  "soc_ico": {
    "nb_l2_shared_banks": 4,
    "gv_class": "pulp.Soc_Ico_v2.Soc_Ico",
    "vp_class": null,
    "vp_comps": [
      "ll_ico",
      "hb_ico"
    ],
    "vp_ports": [
      "debug",
      "axi_slave",
      "apb",
      "axi_master",
      "l2",
      "l2_shared_0",
      "l2_shared_1",
      "l2_shared_2",
      "l2_shared_3"
