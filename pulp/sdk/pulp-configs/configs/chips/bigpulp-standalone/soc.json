    "adv_dbg_unit"
  ],
  "vp_ports": [
    "cluster_clock",
    "jtag0"
  ],
  "vp_bindings": [
    [
      "self->cluster_clock",
      "cluster->clock"
    ],
    [
      "self->jtag0",
      "adv_dbg_unit->jtag"
    ],
    [
      "axi_ico->cluster",
      "cluster->input"
    ],
