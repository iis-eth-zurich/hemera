    "version": 1
  },
  "cluster": {
    "includes": [
      "chips/bigpulp-standalone/cluster_0.json"
    ],
    "vp_ports": [
      "soc",
      "clock",
      "input"
    ]
  },
  "adv_dbg_unit": {
    "includes": [
      "ips/adv_dbg_unit/adv_dbg_unit.json"
    ],
