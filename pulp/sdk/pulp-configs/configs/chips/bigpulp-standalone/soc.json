      ],
      [
        "hb_ico->out_1",
        "self->l2_shared_1"
      ],
      [
        "hb_ico->out_2",
        "self->l2_shared_2"
      ],
      [
        "hb_ico->out_3",
        "self->l2_shared_3"
      ]
    ],
    "ll_ico": {
      "includes": [
        "ips/interco/router.json"
      ],
      "mappings": {
        "apb": {
          "base": "0x1A100000",
          "size": "0x00100000"
        },
        "axi_master": {
          "base": "0x10000000",
          "size": "0x400000"
        },
        "l2": {
          "base": "0x1C000000",
          "size": "0x00080000",
          "remove_offset": "0x1C000000"
        }
      },
      "vp_ports": [
        "apb",
        "l2",
        "axi_master",
        "input"
      ]
    },
    "hb_ico": {
      "includes": [
        "ips/interco/interleaver.json"
      ],
      "nb_slaves": 4,
      "interleaving_bits": 2,
      "remove_offset": "0x1C000000",
      "vp_ports": [
        "out_0",
        "out_1",
        "out_2",
        "out_3"
      ]
    }
  },
  "apb_ico": {
    "includes": [
      "ips/interco/router.json"
    ],
    "mappings": {
      "stdout": {
        "base": "0x1A10F000",
        "size": "0x00001000",
        "remove_offset": "0x1A10F000"
